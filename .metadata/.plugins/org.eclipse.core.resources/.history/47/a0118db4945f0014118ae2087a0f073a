package com.decker.smrsremote;

public class StarMadeServer {
	String serverName = "";
	String serverIP = "";

	void StarmadeServer(String Name, String IP) {
		serverName = Name;
		serverIP = IP;
	}

	public String getName() {
		return serverName;
	}

	public String getIP() {
		return serverIP;
	}
}
